{
  "version": 2,
  "functions": {
    "api/[...route].php": {
      "runtime": "vercel-php@0.7.1",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/public/build/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/(.*\\.(css|js|ico|png|jpg|jpeg|svg|gif|webp|woff|woff2|ttf|eot))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/[...route].php"
    }
  ],
  "env": {
    "VERCEL": "1",
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "stderr",
    "LOG_LEVEL": "debug",
    "SESSION_DRIVER": "cookie",
    "CACHE_DRIVER": "array",
    "QUEUE_CONNECTION": "sync"
  }
}